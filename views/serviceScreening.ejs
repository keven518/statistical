    <script type="text/javascript">
        $(function(){
            $('.ser_box1').find('.s_scr_sBtn').click(function(){
                $('.ser_box1').css('display','none');
                $('.ser_box2').css('display','block');
            });
            $('.ser_box2').find('.s_scr_sBtn').click(function(){
                $('.ser_box2').css('display','none');
                $('.ser_box1').css('display','block');
            });
            $('.s_hover').click(function(){
                $(this).attr('class','s_active').siblings().attr('class','s_hover');
                if($('#ser_c_box').length > 0){
                    $('#ser_c_box').text('服务类型：'+$(this).text());
                }else if($('.s_screening_tit_txt').next().length > 0 && $('.s_screening_tit_txt').next().attr('id')!='ser_c_box'){
                    $('.s_screening_tit_txt').next().before('<span class="s_sx_nx" id="ser_c_box">服务类型：'+$(this).text()+'</span>')
                }else{
                    $('.s_screening_tit_txt').after('<span class="s_sx_nx" id="ser_c_box">服务类型：'+$(this).text()+'</span>');
                }
            });
            $('.t_hover').click(function(){
                $(this).attr('class','s_active').siblings().attr('class','s_hover');
                if($('#time_c_box').length > 0){
                    $('#time_c_box').text('服务类型：'+$(this).text());
                }else{
                    if($('#ser_c_box').length > 0){
                        $('#ser_c_box').after('<span class="s_sx_nx" id="time_c_box">服务类型：'+$(this).text()+'</span>');
                    }else{
                    $('.s_screening_tit_txt').after('<span class="s_sx_nx" id="time_c_box">服务类型：'+$(this).text()+'</span>');}
                }
            });
            $('.c_hover').click(function(){
                $(this).attr('class','s_active').siblings().attr('class','s_hover');
                if($('#cs_c_box').length > 0){
                    $('#cs_c_box').text('服务类型：'+$(this).text());
                }else{

                    if($('#time_c_box').length > 0){
                        $('#time_c_box').after('<span class="s_sx_nx" id="cs_c_box">服务类型：'+$(this).text()+'</span>');
                    }else if($('#ser_c_box').length > 0){
                        $('#ser_c_box').after('<span class="s_sx_nx" id="cs_c_box">服务类型：'+$(this).text()+'</span>');
                    }else{
                    $('.s_screening_tit_txt').after('<span class="s_sx_nx" id="cs_c_box">服务类型：'+$(this).text()+'</span>');}
                }
            });

            $.getJSON('kv_getServiceInfo', function (res) {
                if(res.msg == 'success'){
                    $.each( res.result, function(i,n){
                      $('#kv_tab').find('tbody').append('<tr class="even pointer">\
                                    <td class=" "><a href="serviceList">'+n.servicename+'</a></td>\
                                    <td class=" "><a href="serviceList">'+n.channelname+'</a></td>\
                                    <td class=" "><a href="serviceList">'+n.type+'</a></td>\
                                    <td class=" "><a href="serviceList">'+n.createtime+'</a></td>\
                                    <td class=" "><a href="serviceList">'+n.owner+'</a></td>\
                                    <td class="a-right a-right "><a href="serviceList">'+n.phone+'</a></td>\
                                    <td class="a-right a-right "><a href="serviceList">'+n.email+'</a></td>\
                                    <td class="a-right a-right "><a href="serviceList">'+n.count+'</a></td>\
                                    <td class="a-right a-right "><a href="serviceList">'+n.serviceid+'</a></td>\
                                            </tr>');
                    });
                }else{
                    console.log(res.msg);
                }
            });
        })
    </script>
