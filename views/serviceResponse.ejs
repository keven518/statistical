    <script>
        $(function () {

            $.getJSON('kv_getUsedServiceInfoByRespTime', function (res) {
                if(res.msg == 'success'){
                    $('#data_total').text('共'+res.result.length+'p11条记录');
                    $.each( res.result, function(i,n){
                      $('#kv_tab').find('tbody').append('<tr class="even pointer">\
                                    <td class=" ">'+n.servicename+'</td>\
                                    <td class=" ">'+n.channelname+'</td>\
                                    <td class=" ">'+n.calltime+'</td>\
                                    <td class=" ">'+n.callchannel+'</td>\
                                    <td class=" ">'+n.resulttype+'</td>\
                                    <td class="a-right a-right ">'+n.restime+'</td>\
                                            </tr>');
                    });
                      $("div.pagination").jPages({
                        containerID  : "itemContainer",
                        perPage      : 15,
                        startPage    : 1,
                        startRange   : 1,
                        midRange     : 4,
                        endRange     : 1
                      });
                }else{
                    console.log(res.msg);
                }
            });

        });
    </script>
    
